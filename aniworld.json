{
    "name": "AniWorld",
    "version": "1.0.0",
    "author": {
        "name": "sobet",
        "website": "https://google.com"
    },
    "iconURL": "https://raw.githubusercontent.com/hamzenis/SoraSource/refs/heads/main/assests/aniworld.png",
    "stream": "HLS2",
    "language": "DE",
    "extractor": "dub-sub",
    "module": [
        {
            "search": {
                "url": "https://aniworld.to/animes",
                "parameter": "",
                "documentSelector": "div.genre",
                "title": "a",
                "image": {
                    "url": "img",
                    "attribute": "src"
                },
                "href": "a"
            },
            "featured": {
                "url": "https://aniworld.to/beliebte-animes",
                "documentSelector": "div.seriesListContainer row col-md-15 col-sm-3 col-xs-6",
                "title": "h3",
                "image": {
                    "url": "img",
                    "attribute": "src"
                },
                "href": "a"
            },
            "details": {
                "baseURL": "https://aniworld.to",
                "aliases": {
                    "selector": "div.post-title h1",
                    "attribute": "text"
                },
                "synopsis": "div.entry-content",
                "airdate": "div.post-meta span:contains(Aired) + span",
                "stars": "div.post-meta span:contains(Rating) + span"
            },
            "episodes": {
                "selector": "div.eplister ul li a",
                "order": "reversed",
                "pattern": ""
            }
        }
    ]
}